<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.fengzhi.web.entity.User" table="tb_user" catalog="mgrdb">
		<id name="userId" type="java.lang.Long">
			<column name="user_id" />
			<generator class="identity" />
		</id>
		<property name="userName" type="string">
			<column name="user_name" length="20" not-null="true">
			</column>
		</property>
		<property name="userNo" type="string">
			<column name="user_no" length="20">
			</column>
		</property>
		<property name="userPassword" type="string">
			<column name="user_password" length="32">
			</column>
		</property>
		<property name="borth" type="timestamp">
			<column name="borth" length="20">
			</column>
		</property>
		<property name="gender" type="string">
			<column name="gender" length="2">
			</column>
		</property>
		<property name="serialNo" type="string">
			<column name="serial_no" length="20">
			</column>
		</property>
		<property name="telphone" type="string">
			<column name="telphone" length="11">
			</column>
		</property>
		<property name="address" type="string">
			<column name="address" length="100">
			</column>
		</property>
		<property name="email" type="string">
			<column name="email" length="50" not-null="true">
				<comment>E-mail</comment>
			</column>
		</property>
		<property name="workExperience" type="string">
			<column name="work_experience" length="20">
			</column>
		</property>
		<property name="position" type="string">
			<column name="position" length="20">
			</column>
		</property>
		<property name="politicalAffiliation" type="string">
			<column name="political_affiliation" length="20">
			</column>
		</property>
		<property name="graduation" type="string">
			<column name="graduation" length="100">
			</column>
		</property>
		<property name="userType" type="int">
			<column name="user_type" not-null="true">
			</column>
		</property>
		<property name="introduce" type="string">
			<column name="introduce" length="65535">
			</column>
		</property>
		<property name="websiteUrl" type="string">
			<column name="website_url" length="100">
			</column>
		</property>
		<property name="photo" type="string">
			<column name="photo" length="100">
			</column>
		</property>
		<property name="orgId" type="java.lang.Long">
			<column name="org_id">
			</column>
		</property>
		<property name="flag" type="char">
			<column name="flag" length="1" not-null="true">
			</column>
		</property>
		<property name="priv" type="string">
			<column name="priv" length="100">
			</column>
		</property>
		<property name="fatherName" type="string">
			<column name="father_name" length="50">
			</column>
		</property>
		<property name="fatherTel" type="string">
			<column name="father_tel" length="11">
			</column>
		</property>
		<property name="motherName" type="string">
			<column name="mother_name" length="50">
			</column>
		</property>
		<property name="motherTel" type="string">
			<column name="mother_tel" length="11">
			</column>
		</property>
		<property name="actives" type="java.lang.Integer">
			<column name="actives" />
		</property>
		<property name="cardId" type="string">
			<column name="cardId" length="20"/>
		</property>
		<property name="isOpenSms" type="java.lang.Character">
			<column name="isOpenSms" length="1"/>
		</property>
		<property name="sign" type="string">
			<column name="sign" length="20" />
		</property>
		<property name="groupId" type="java.lang.Integer">
			<column name="group_id" />
		</property>
		<property name="createDate" type="timestamp">
			<column name="create_date" length="19"/>
		</property>
		<property name="modifyDate" type="timestamp">
			<column name="modify_date" length="19">
			</column>
		</property>
		<property name="nickname" type="string">
			<column name="nickname" length="20" />
		</property>
		<property name="namestory" type="string">
			<column name="namestory" length="65535" />
		</property>
		<property name="bornstory" type="string">
			<column name="bornstory" length="65535" />
		</property>
		<property name="registerFlag" type="java.lang.Character">
			<column name="registerFlag" length="1" />
