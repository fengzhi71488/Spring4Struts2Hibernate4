			<column name="smsDate" length="19">
			</column>
		</property>
		<property name="smsCardCount" type="java.lang.Short">
			<column name="smsCardCount"/>
		</property>
		<property name="bdUserId" type="string">
			<column name="bd_user_id" length="64">
			</column>
		</property>
		<property name="bdChannelId" type="string">
			<column name="bd_channel_id" length="64">
			</column>
		</property>

		<property name="osType" type="char">
			<column name="os_type" length="1" />
		</property>

		<property name="deviceSerialId" type="string">
			<column name="device_serial_id" length="100" />
		</property>
		<property name="vcode" type="java.lang.Integer">
			<column name="vcode" />
		</property>
		<property name="vcodeTime" type="timestamp">
			<column name="vcode_time" />
		</property>

		<property name="videoFlag" type="char">
			<column name="video_flag" length="1" />
		</property>

		<property name="videoAccount" type="string">
			<column name="video_account" length="20" />
		</property>

		<property name="videoPassword" type="string">
			<column name="video_password" length="20" />
		</property>
		<property name="gpsId" type="int">
			<column name="gps_id" />
		</property>
		<property name="gpsStatus" type="char">
			<column name="gps_status" length="1" />
		</property>
		<property name="devicetoken" type="string">
			<column name="devicetoken" length="64" />
		</property>
		<property name="isManager" type="char">
			<column name="is_manager" length="1" />
		</property>
		<property name="appFlag" type="char">
			<column name="app_flag" length="1" />
		</property>
		<property name="recordFlag" type="char">
			<column name="record_Flag" length="1" />
		</property>
	</class>
</hibernate-mapping>
